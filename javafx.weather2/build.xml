<project name="org.netbeans.modules.javafx.weather2" default="netbeans" basedir=".">
    <description>Builds, tests, and runs the project org.netbeans.modules.javafx.weather2.</description>
    <import file="nbproject/build-impl.xml"/>
    <target name="zip-weather">
        <mkdir dir="build/classes/org/netbeans/modules/javafx/weather2/resources/zip"/>
        <zip destfile="build/classes/org/netbeans/modules/javafx/weather2/resources/zip/WeatherFXApplicationProject.zip"
             basedir="resources/WeatherFX" update="true"/>
    </target>
    
    <target name="init" depends="basic-init,files-init,build-init,zip-weather"/>
</project>
